<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gredient_background"
    android:outlineProvider="paddedBounds"
    tools:context=".MainActivity7">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@drawable/wave__1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="1000dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="80dp"
            android:layout_marginRight="80dp"
            android:layout_marginBottom="80dp"
            android:outlineProvider="background"
            android:text="shop categories"
            android:textAlignment="center"
            android:textColor="@color/mycolor3"
            android:textSize="44sp"
            android:textStyle="bold" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="375dp"
        android:layout_height="74dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="240dp"
        android:background="@drawable/edit_round"
        android:contextClickable="true"
        android:elegantTextHeight="true"
        android:freezesText="true"
        android:text="restaurants"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="375dp"
        android:layout_height="74dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="136dp"
        android:background="@drawable/edit_round"
        android:contextClickable="true"
        android:elegantTextHeight="true"
        android:freezesText="true"
        android:text="clothe_center"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="375dp"
        android:layout_height="74dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/edit_round"
        android:contextClickable="true"
        android:elegantTextHeight="true"
        android:freezesText="true"
        android:text="super_market1"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="236dp"
        android:layout_marginBottom="252dp"
        android:backgroundTint="@color/light_blue1"
        android:text="extend_all"
        android:textColor="@color/mycolor2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="236dp"
        android:layout_marginTop="512dp"
        android:backgroundTint="@color/light_blue1"
        android:text="extend_all"
        android:textColor="@color/mycolor2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="236dp"
        android:layout_marginTop="408dp"
        android:backgroundTint="@color/light_blue1"
        android:text="extend_all"
        android:textColor="@color/mycolor2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>
